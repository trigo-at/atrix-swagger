{
  "name": "@trigo/atrix-swagger",
  "version": "2.1.0",
  "engines": {
    "node": ">=7.6.0"
  },
  "config": {
    "loadOnStart": true
  },
  "description": "Swagger API Language intigration ",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --timeout 30000 $(find . -name '*.specs.js' -not -path './node_modules/*')",
    "lint": "eslint lib specs",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "precommit": "lint-staged",
    "prettify": "prettier --write '**/*.{js,jsx}'"
  },
  "keywords": [
    "atrix",
    "swagger"
  ],
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier",
      "git add"
    ]
  },
  "author": "Christian Hubinger <christian@trigo.at>",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.5.0",
    "joi": "^14.3.0",
    "joi-date-extensions": "^1.0.2",
    "ramda": "^0.26.1",
    "swagger-parser": "^6.0.2"
  },
  "devDependencies": {
    "@trigo/atrix": "^6.1.0",
    "chai": "^4.0.2",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-trigo": "^6.2.0",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^8.1.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.11.0",
    "prettier": "^1.15.3",
    "supertest": "^3.0.0",
    "uuid": "^3.1.0"
  }
}
